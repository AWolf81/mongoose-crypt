{
  "name": "mongoose-encrypt",
  "version": "0.0.1",
  "description": "",
  "main": "mongoose-encrypt.js",
  "scripts": {
    "dev": "concurrently \"npm run compile:watch\" \"npm run lint\"",
    "lint": "esw ./src ./features -w",
    "compile": "npm run clean && babel -d lib/ src/",
    "clean": "rimraf lib",
    "compile:watch": "chokidar ./src/*.js -c \"npm run compile\"",
    "test:mocha": "npm run compile && mocha --compilers js:babel-core/register --reporter spec",
    "test": "npm run compile && cucumber-js",
    "cucumber": "cucumber-js",
    "cover": "nyc cucumber-js",
    "watch": "concurrently \"npm test\" \"chokidar ./features/*.* -c \"npm test\"\""
  },
  "keywords": [],
  "author": "AWolf81",
  "license": "MIT",
  "dependencies": {
    "crypto-js": "^3.1.9-1",
    "dotty": "^0.1.0",
    "mongodb": "^3.0.10",
    "mongoose": "^5.1.4"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.3",
    "babel-preset-env": "^1.7.0",
    "chai": "^4.1.2",
    "chokidar-cli": "^1.2.0",
    "concurrently": "^3.5.1",
    "cucumber": "^4.2.1",
    "eslint": "^4.19.1",
    "eslint-config-standard": "^11.0.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-promise": "^3.8.0",
    "eslint-plugin-standard": "^3.1.0",
    "eslint-watch": "^3.1.5",
    "mocha": "^5.2.0",
    "nodemon": "^1.17.5",
    "nyc": "^12.0.2",
    "prettier-eslint": "^8.8.1",
    "rimraf": "^2.6.2"
  },
  "nyc": {
    "include": [
      "**/lib/*.js"
    ],
    "reporter": [
      "lcov",
      "text-summary"
    ]
  }
}
